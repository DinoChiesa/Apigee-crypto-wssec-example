<AssignMessage name="AM-Signed-Response">
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <Set>
    <Headers>
      <Header name='key-identifier-type'>{desired_key_identifier_type}</Header>
    </Headers>

    <Payload contentType="text/xml">{signed_soap}
</Payload>
    <ReasonPhrase>OK</ReasonPhrase>
    <StatusCode>200</StatusCode>
  </Set>

  <AssignTo>response</AssignTo>
</AssignMessage>
